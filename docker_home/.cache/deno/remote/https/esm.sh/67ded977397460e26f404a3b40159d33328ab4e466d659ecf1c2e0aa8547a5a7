function f(s,e){s.length>e.length&&([s,e]=[e,s]);let i=Array.from({length:s.length+1},(t,o)=>+o);for(let t=0;t<e.length;t++){let o=[t+1];for(let r=0;r<s.length;r++){let l=s[r],n=e[t];l===n?o.push(i[r]):o.push(1+Math.min(i[r],i[r+1],o[o.length-1]))}i=o}return i[i.length-1]}var d=class extends Error{constructor(e,{givenWord:i,givenWords:t,possibleWords:o}){super(e),this.givenWord=i,this.givenWords=t,this.possibleWords=o}};function h(s){var{givenWord:e,givenWords:i,possibleWords:t,caseSensitive:o,autoThrow:r,suggestionLimit:l}={suggestionLimit:1/0,...s};if(i instanceof Array){let n={};for(let g of i)n[g]=h({...s,givenWord:g,givenWords:void 0});return n}if(o||(t=t.map(n=>n.toLowerCase()),e=e.toLowerCase()),!t.includes(e)&&r){let n=h({givenWord:e,possibleWords:t,caseSensitive:o,suggestionLimit:l});throw l==1&&n.length>0?new d(`For ${JSON.stringify(e)}, did you mean ${JSON.stringify(n[0])}?`,{givenWord:e,possibleWords:n}):new d(`For ${JSON.stringify(e)}, did you mean one of ${JSON.stringify(n)}?`,{givenWords:i,possibleWords:n})}return[...t].sort((n,g)=>f(e,n)-f(e,g)).slice(0,l)}export{d as DidYouMeanError,h as didYouMean};
