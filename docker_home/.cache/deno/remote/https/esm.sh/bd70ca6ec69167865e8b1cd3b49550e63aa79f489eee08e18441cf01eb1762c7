var o=(n,s)=>{let i=-1,t=[...n];for(let e of t){i++;let f;if(s&&typeof(f=s(e,i+1,n))=="string"){t[i]=f;continue}if(e=="\\")t[i]="\\\\";else if(e=="`")t[i]="\\`";else if(e=="$")t[i+1]=="{"?t[i]="\\$":t[i]="$";else if(e=="\r")t[i]="\\r";else if(e=="\b"||e=="	"||e==`
`||e=="\v"||e=="\f")t[i]=e;else if(e.codePointAt(0)<127)t[i]=e;else if(/\$|\p{ID_Continue}/u.test(e))t[i]=e;else{let l=JSON.stringify(e);l.length>4?t[i]=l.slice(1,-1):t[i]=e}}return"`"+t.join("")+"`"};export{o as escapeJsString};
